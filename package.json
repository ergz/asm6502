{
  "name": "asm6502",
  "displayName": "asm6502-syntax",
  "description": "6502 ASM Syntax Highlighting",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "asm6502",
      "aliases": ["asm6502", "asm6502"],
      "extensions": [".s",".asm"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "asm6502",
      "scopeName": "source.asm",
      "path": "./syntaxes/dasm.tmLanguage"
    }]
  }
}
